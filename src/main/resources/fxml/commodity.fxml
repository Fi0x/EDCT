<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<HBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.fi0x.edct.controller.Commodity" alignment="CENTER">
    <Button mnemonicParsing="false" onAction="#previousCommodity" text="Previous Commodity"/>
    <VBox>
        <Label fx:id="lblCommodity" text="?Commodity?"/>
        <Label fx:id="lblProfit" text="Profit: ---"/>
    </VBox>
    <Button mnemonicParsing="false" onAction="#nextCommodity" text="Next Commodity"/>
    <stylesheets>
        <URL value="@../css/commodity.css"/>
    </stylesheets>
</HBox>

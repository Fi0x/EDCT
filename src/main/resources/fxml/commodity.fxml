<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<HBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.fi0x.edct.controller.Commodity" alignment="CENTER">
    <Button fx:id="btnPrevComm" mnemonicParsing="false" onAction="#previousCommodity" text="Previous Commodity" disable="true"/>
    <VBox alignment="CENTER">
        <Label fx:id="lblCommodity" text="---"/>
        <Label fx:id="lblProfit" text="Profit: ---"/>
    </VBox>
    <Button fx:id="btnNextComm" mnemonicParsing="false" onAction="#nextCommodity" text="Next Commodity" disable="true"/>
    <stylesheets>
        <URL value="@../css/commodity.css"/>
    </stylesheets>
</HBox>
